@model object
<div class="form-group">
    @Html.LabelFor(m => m, new { @class = "col-md-3 control-label" })
    <div class="col-md-9">
        @Html.DropDownListFor(m => m, (SelectList)ViewBag.Items, new { @class = "form-control"})
        @Html.ValidationMessageFor(m => m, null, new { @class = "help-block" })
    </div>
</div>